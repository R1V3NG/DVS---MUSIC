<Window x:Class="DVS_Player.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DVS_Player"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Slider Name ="sMusic" HorizontalAlignment="Center" Margin="0,336,0,0" VerticalAlignment="Top" Width="780" Thumb.DragStarted="sMusic_DragStarted" Thumb.DragCompleted="sMusic_DragCompleted"/>
        <Label Name="lStart" Content="Label" HorizontalAlignment="Left" Margin="10,359,0,0" VerticalAlignment="Top"/>
        <Label Name ="lEnd" Content="Label" HorizontalAlignment="Left" Margin="752,359,0,0" VerticalAlignment="Top"/>
        <Button Name ="bOpen" Content="Открыть" HorizontalAlignment="Left" Margin="103,0,0,0" VerticalAlignment="Center" Height="71" Width="105" Click="bOpen_Click"/>
        <Button Name ="bPause" Content="Pause" HorizontalAlignment="Left" Margin="457,0,0,0" VerticalAlignment="Center" Height="106" Width="135" Click="Button_Click"/>
    </Grid>
</Window>
